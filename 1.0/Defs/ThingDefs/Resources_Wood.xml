<Defs>
 
<!-- Plant & Vegetable Garden Tree --> 
   
 	<ThingDef ParentName="WoodLogBase">
		<defName>Bamboo</defName> <!-- SG: 0.5-0.85  W: 500-850  H: 1410-1610 -->
		<label>raw bamboo</label>
		<description>Raw bamboo. When processed it becomes useful for building structures, weapons, and even simple prosthetics. Bamboo is a perishable, fast growing grass.</description>
		<graphicData>
			<texPath>Things/Resources/Bamboo/bamboo</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<statBases>
			<Mass>0.6</Mass>
			<MarketValue>1.2</MarketValue>
			<DeteriorationRate>4</DeteriorationRate>
		</statBases>
		<stuffProps>
			<stuffAdjective>bamboo</stuffAdjective>
			<commonality>0.5</commonality>
			<color>(248,218,158)</color>
			<statFactors>
				<MaxHitPoints>0.5</MaxHitPoints>
				<Beauty>1.25</Beauty>
				<WorkToMake>0.7</WorkToMake>
				<WorkToBuild>0.7</WorkToBuild>
				<DoorOpenSpeed>1.3</DoorOpenSpeed>
			</statFactors>
		</stuffProps>
		<butcherProducts>
			<LumberBamboo>30</LumberBamboo>
		</butcherProducts>		
	</ThingDef>
  
	<ThingDef ParentName="WoodLogBase">
		<defName>VG_IronWoodLog</defName>
		<label>ironwood logs</label>
		<description>Raw wood from an ironwood tree. When processed it becomes useful for building structures, weapons, and even simple prosthetics. Ironwood is a very durable hardwood.</description>
		<graphicData>
			<texPath>Things/Item/IronWoodLog</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<statBases>
			<Mass>0.8</Mass>
			<MarketValue>1.9</MarketValue>
			<DeteriorationRate>1</DeteriorationRate>
			<SharpDamageMultiplier>0.7</SharpDamageMultiplier>
		</statBases>
		<stuffProps>
			<stuffAdjective>ironwood</stuffAdjective>
			<commonality>0.6</commonality>
			<color>(100,100,100)</color>
			<statFactors>
				<MaxHitPoints>0.75</MaxHitPoints>
				<Beauty>1.2</Beauty>
				<Flammability>0.75</Flammability>
				<WorkToMake>1.25</WorkToMake>
				<WorkToBuild>1.25</WorkToBuild>
				<DoorOpenSpeed>1.0</DoorOpenSpeed>
			</statFactors>
		</stuffProps>
		<butcherProducts>
			<LumberIronwood>30</LumberIronwood>
		</butcherProducts>
	</ThingDef>   
  
