<Defs> 
	
<!-- Plant & Vegetable Garden Wood Tree -->	
	
	<ThingDef ParentName="WoodLumberBase">
		<defName>LumberBamboo</defName>
		<label>bamboo lumber</label>
		<description>Processed bamboo lumber. Mostly useful for building long-lasting, simple structures and furniture. Bamboo is a perishable, fast growing grass.</description>
		<graphicData>
			<color>(248,218,158)</color>
		</graphicData>
		<statBases>
			<Mass>0.3</Mass>
			<MarketValue>1.2</MarketValue>
			<DeteriorationRate>5</DeteriorationRate>
		</statBases>
		<stuffProps>
			<stuffAdjective>bamboo</stuffAdjective>
			<commonality>0.5</commonality>
			<color>(248,218,158)</color>
			<statFactors>
				<MaxHitPoints>0.5</MaxHitPoints>
				<Beauty>1.25</Beauty>
				<WorkToMake>0.7</WorkToMake>
				<WorkToBuild>0.7</WorkToBuild>
				<DoorOpenSpeed>1.3</DoorOpenSpeed>
			</statFactors>
		</stuffProps>
	</ThingDef>
	
	<ThingDef ParentName="WoodLumberBase">
		<defName>VG_LumberIronwood</defName>
		<label>ironwood lumber</label>
		<description>Processed ironwood lumber. Mostly useful for building long-lasting, simple structures and furniture. Ironwood is a very durable hardwood.</description>
		<graphicData>
			<color>(100,100,100)</color>
		</graphicData>
		<statBases>
			<Mass>0.4</Mass>
			<MarketValue>2.6</MarketValue>
			<DeteriorationRate>1</DeteriorationRate>
			<StuffPower_Armor_Sharp>0.6</StuffPower_Armor_Sharp>
			<StuffPower_Armor_Blunt>0.6</StuffPower_Armor_Blunt>
			<StuffPower_Armor_Heat>0.5</StuffPower_Armor_Heat>
			<SharpDamageMultiplier>0.7</SharpDamageMultiplier>
		</statBases>
		<stuffProps>
			<stuffAdjective>ironwood</stuffAdjective>
			<commonality>0.75</commonality>
			<color>(100,100,100)</color>
			<statFactors>
				<MaxHitPoints>0.75</MaxHitPoints>
				<Beauty>1.2</Beauty>
				<Flammability>0.75</Flammability>
				<WorkToMake>1.25</WorkToMake>
				<WorkToBuild>1.25</WorkToBuild>
				<DoorOpenSpeed>1.0</DoorOpenSpeed>
			</statFactors>			
		</stuffProps>
	</ThingDef>	

